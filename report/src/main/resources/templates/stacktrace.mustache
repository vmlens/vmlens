{{>header.mustache}}

<!-- stacktrace.mustache -->
<div class="{{containerTyp}}">
    {{#issues}}
        <div class="row text-left">
            {{#imagePath}}
                <img src="../{{{imagePath}}}"/>
            {{/imagePath}}
            {{#hasLink}}<a href="{{link}}"> {{{nameWithHtml}}} </a> {{/hasLink}}
            {{^hasLink}} {{{nameWithHtml}}} {{/hasLink}}
        </div>
        {{#children}}
            <div class="row text-left">
                <div class="col-sm-1"></div>
                <div class="col-sm-11 text-left">
                    {{#imagePath}}
                        <img src="../{{{imagePath}}}"/>
                    {{/imagePath}}
                    {{{nameWithHtml}}} </div>
            </div>
            {{#children}}
                <div class="row text-left">
                    <div class="text-justify col-sm-2"></div>
                    <div class="col-sm-10 text-left" style="font-size : 16px">
                        {{#imagePath}}
                            <img src="../{{{imagePath}}}" width="16"/>
                        {{/imagePath}}
                        {{{nameWithHtml}}} </div>
                </div>
            {{/children}}
        {{/children}}
    {{/issues}}
</div>
{{>footer.mustache}}