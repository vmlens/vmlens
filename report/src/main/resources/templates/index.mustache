{{>header.mustache}}

<!-- index.mustache -->

<div class="container-fluid">

    <table class="table table-striped">
        <thead>
            <tr>
                <th>#</th>
                <th>Name</th>
                <th>Count</th>
                <th>Result</th>
            </tr>
        </thead>
        {{#loops}}
            <tr>
                <td>{{index}}</td>
                <td><a href="{{link}}">{{name}}</a></td>
                <td>{{count}}</td>
                <td {{{resultStyle}}}>{{{resultText}}}</td>
            </tr>
        {{/loops}}
    </table>
</div>
{{>footer.mustache}}