{{>header.mustache}}

<!-- run.mustache -->

<div class="container-fluid">
    <div class="row">
        <table class="table table-striped text-start">
            <thead class="sticky-head">
                <tr>
                    <th colspan="2">
                        <div style="display:flex; justify-content:space-between; align-items:center;">
                            <span><a href="index.html">Overview</a>/{{runName}}</span>
                            <span>State</span>
                            <a href="https://vmlens.com/docs/report/state/" title="Help">Help</a>
                        </div>
                    </th>
                </tr>
                <tr>
                    <th>Method or Lock</th>
                    <th>State</th>
                </tr>
            </thead>
            {{#elements}}
                <tr>
                    <td {{{css}}}>{{label}}</td>
                    <td>
                        <ul>
                        {{#state}}
                         <li><a href="{{link}}"> {{label}}{{idLabel}}</a></li>
                        {{/state}}
                        </ul>

                    </td>
                </tr>
            {{/elements}}
            <tfoot>
                <tr>
                    <td colspan="2" style="text-align: right;"><a href="https://vmlens.com/docs/report/state/">Help</a></td>
                </tr>
            </tfoot>
        </table>
    </div>
</div>
{{>footer.mustache}}